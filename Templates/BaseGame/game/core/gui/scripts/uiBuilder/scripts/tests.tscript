
function testFBFunc2()
{
   %window = GuiFormBuilder::createWindow("Test Alt Window", "", "500 400");
   %splitContainer = %window.createSplitContainer();
   
   %tree = %splitContainer-->panel1.createTreeView();
   %book = %splitContainer-->panel2.createTabBook();
   
   %page = %book.createTabPage("Test Page");
   
   %mainStack = %page.createScrollStack();
   
   %rollout = %mainStack.createRollout("Test Rollout");
   %rolloutStack = %rollout.createStack();
   
   %rolloutStack.createTextField("A Test Field", "AAAAAAAAA");
   %propField = %rolloutStack.createTextPropertyField("Another Test Field", "$GUIFormBuilder::edgePad");
   
   %pageTwo = %book.createTabPage("Other Page");
}

function UIBuilder::test1()
{
   %window = UIBuilder::Window("Test Window", "50 50", "200 500", true);

      UIBuilder::Rollout("Test Rollout");
      
         UIBuilder::TextField("A Test Field", "AAAAAAAAAAA");
         
      UIBuilder::End();
      
      UIBuilder::TextField("Indented Field", "MMMMMKAAAAAY");

   UIBuilder::End();
   
   MainMenuGui.add(%window);
}

