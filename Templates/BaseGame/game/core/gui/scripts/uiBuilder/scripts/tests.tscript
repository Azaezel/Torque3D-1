function UIBuilder::test3()
{
   %layout = UIBuilder::FrameSet(MainMenuGui.extent);
   UIBuilder::End();
   
   %windowA = UIBuilder::Window("Test Window", "50 50", "300 500", true);
      UIBuilder::Rollout("Test Rollout A");
      
         UIBuilder::TextField("A Test Field", "AAAAAAAAAAA");
         
      UIBuilder::End();
   UIBuilder::End();
   
   %windowB = UIBuilder::Window("Another Test Window", "350 50", "300 500", true);
      UIBuilder::Rollout("Test Rollout B");
      
         UIBuilder::TextField("Another Field", "BBBBBBBBBBB");
         
      UIBuilder::End();
   UIBuilder::End();
   
   MainMenuGui.add(%layout);
   MainMenuGui.add(%windowA);
   MainMenuGui.add(%windowB);
}

function UIBuilder::test4()
{
   %layout = UIBuilder::LayoutContainer("0 0", MainMenuGui.extent.x/2 SPC MainMenuGui.extent.y);
   UIBuilder::End();
   
   %tabbook = %layout-->layoutTabbook;
   
   UIBuilder::Set(%tabbook);
   
      %tabA = UIBuilder::TabPage("Test A");
      %tabA.className = "UIBldrLayoutTab";
      UIBuilder::End();
      
      %tabB = UIBuilder::TabPage("Test B");
      %tabB.className = "UIBldrLayoutTab";
      UIBuilder::End();
      
      %tabC = UIBuilder::TabPage("Test C");
      %tabC.className = "UIBldrLayoutTab";
      UIBuilder::End();
      %tabbook.dock(%tabC, "right");
   
   UIBuilder::End();
   
   MainMenuGui.add(%layout);
}

function UIBuilder::test5()
{
   UIBuilder::test4();
   UIBuilder::test1();
}